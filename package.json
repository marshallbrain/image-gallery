{
  "name": "electron-react-template",
  "version": "0.0.1",
  "description": "Personal template based off of secure-electron-template by reZach",
  "private": true,
  "homepage": "./",
  "main": "build/prod.main.js",
  "scripts": {
    "build": "concurrently \"npm run build:render\" \"npm run build:main\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./build-utils/configs/webpack.prod.main.js",
    "build:render": "cross-env NODE_ENV=production webpack --config ./build-utils/configs/webpack.prod.renderer.js",
    "dev": "npm run dev:render",
    "dev:main": "cross-env NODE_ENV=development electron -r ./build-utils/scripts/BabelRegister src/main.ts",
    "dev:render": "cross-env NODE_ENV=development webpack serve --config ./build-utils/configs/webpack.dev.renderer.js",
    "package": "webpack --config ./build-utils/configs/webpack.cleanup.js && npm run build && electron-builder build --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "{Project Name}",
    "appId": "com.marshalldbrain.projectname",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "zip"
      ]
    }
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "better-sqlite3": "^7.4.5",
    "dot-prop": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "sharp": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@mui/material": "^5.2.4",
    "@mui/icons-material": "^5.2.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@types/better-sqlite3": "^7.4.2",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/sharp": "^0.29.4",
    "@types/lodash": "^4.14.178",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "react-refresh": "^0.11.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "process": "^0.11.10",
    "image-size": "^1.0.1",
    "lodash": "^4.17.21",
    "exifreader": "^4.2.0",
    "uuid": "^8.3.2"
  }
}
